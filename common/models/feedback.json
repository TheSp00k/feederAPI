{
  "name": "feedback",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "productid": {
      "type": "number"
    },
    "commentheader": {
      "type": "string"
    },
    "commentcontent": {
      "type": "string"
    },
    "created": {
      "type": "date"
    },
    "totalratingscore": {
      "type": "number"
    },
    "approved": {
      "type": "boolean"
    },
    "purchased": {
      "type": "boolean"
    },
    "rejected": {
      "type": "boolean"
    },
    "clientid": {
      "type": "string"
    },
    "customerid": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "ratings": {
      "type": "hasMany",
      "model": "rating",
      "foreignKey": "feedbackid"
    },
    "client": {
      "type": "belongsTo",
      "model": "client",
      "foreignKey": "clientid"
    },
    "product": {
      "type": "belongsTo",
      "model": "product",
      "foreignKey": "productid"
    },
    "customer": {
      "type": "belongsTo",
      "model": "customer",
      "foreignKey": "customerid"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "feedbackAdmin",
      "permission": "ALLOW",
      "property": "*"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "sendFeedback"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "*"
    }
  ],
  "methods": {}
}
