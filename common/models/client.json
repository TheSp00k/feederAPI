{
	"name": "client",
	"base": "PersistedModel",
	"idInjection": true,
	"options": {
		"validateUpsert": true
	},
	"properties": {
		"name": {
			"type": "string"
		},
		"companycode": {
			"type": "string"
		},
		"licenseid": {
			"type": "string"
		},
		"email": {
			"type": "string"
		},
		"requesttime": {
			"type": "date"
		},
		"requestdelay": {
			"type": "number"
		},
		"sendrequests": {
			"type": "boolean"
		},
		"displaywidget": {
			"type": "boolean"
		},
		"showheader": {
			"type": "boolean"
		},
		"logourl": {
			"type": "string"
		},
		"domain": {
			"type": "string"
		},
		"appid": {
			"type": "string"
		}
	},
	"hidden": [
		"appid"
	],
	"validations": [],
	"relations": {
		"products": {
			"type": "hasMany",
			"model": "product",
			"foreignKey": "clientid"
		}
	},
	"acls": [
		{
			"accessType": "*",
			"principalType": "ROLE",
			"principalId": "$everyone",
			"permission": "DENY"
		},
		{
			"principalType": "ROLE",
			"principalId": "$authenticated",
			"permission": "ALLOW",
			"property": "*"
		},
		{
			"principalType": "ROLE",
			"principalId": "$everyone",
			"permission": "ALLOW",
			"property": "authAppId"
		}
	],
	"methods": {}
}