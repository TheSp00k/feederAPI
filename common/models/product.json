  },
  "properties": {
    "productnumber": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "created": {
      "type": "date"
    },
    "clientid": {
      "type": "string"
    },
    "photourl": {
      "type": "string"
    },
    "sendrequests": {
      "type": "boolean"
    },
    "showfeedbacks": {
      "type": "boolean"
    }
